<?xml version='1.0' encoding='UTF-8' ?>
<portlet-app version="2.0" id="glasogonbidrag-core-bc-module-portlet"
             xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="
                http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd
                http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

    <portlet>
        <portlet-name>glasogonbidrag-create-invoice</portlet-name>
        <display-name>glasogonbidrag-create-invoice</display-name>
        <portlet-class>com.liferay.faces.bridge.GenericLiferayFacesPortlet</portlet-class>

        <init-param>
            <name>javax.portlet.faces.defaultViewId.view</name>
            <value>/views/create_invoice/view.xhtml</value>
        </init-param>

        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
        </supports>

        <portlet-info>
            <title>glasogonbidrag-create-invoice</title>
            <short-title>glasogonbidrag-create-invoice</short-title>
            <keywords>glasogonbidrag-create-invoice</keywords>
        </portlet-info>
    </portlet>


    <portlet>
        <portlet-name>glasogonbidrag-db-view</portlet-name>
        <display-name>glasogonbidrag-db-view</display-name>
        <portlet-class>com.liferay.faces.bridge.GenericLiferayFacesPortlet</portlet-class>

        <init-param>
            <name>javax.portlet.faces.defaultViewId.view</name>
            <value>/views/db_view/view.xhtml</value>
        </init-param>

        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
        </supports>

        <portlet-info>
            <title>glasogonbidrag-db-view</title>
            <short-title>glasogonbidrag-db-view</short-title>
            <keywords>glasogonbidrag-db-view</keywords>
        </portlet-info>
    </portlet>

</portlet-app>
